
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section style {

}
@section Content
{
   
    <div class="cal-details">
        <div class="container-fluid d-block ">
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 ">
                    <div class="row select-dropdown">
                        <div class="col-xs-11 col-sm-11 col-md-11 col-lg-11 "></div>
                        <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1 ">
                            <div class="form-group">
                                <!--		Show Numbers Of Rows 		-->
                                <select class="form-control" name="state" id="maxRows">
                                    <option value="5000">Show ALL Rows</option>
                                    <option value="5">5</option>
                                    <option value="10">10</option>
                                    <option value="15">15</option>
                                    <option value="20">20</option>
                                    <option value="50">50</option>
                                    <option value="70">70</option>
                                    <option value="100">100</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="cal-details-table table-responsive">
                        <table class="table table-striped   table-hover" id="patientData">
                            <thead class="cal-table-head">
                                <tr>
                                    <th>Caller Type</th>
                                    <th>Caller Name</th>
                                    <th>Contact Number</th>
                                    <th>Pasat Call Records</th>
                                    <th>Patient Past Doctor History</th>
                                    <th>Call Time</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                        <div class="container">
                            <div class="row">
                                <div class="col-xs-10 col-sm-10 col-md-10 col-lg-11"></div>
                                <div class="col-xs-2 col-sm-2 col-md-2 col-lg-1">
                                    <div class='pagination-container'>
                                        <nav>
                                            <ul class="pagination">
                                                <li data-page="prev">
                                                    <span> < <span class="sr-only">(current)</span></span>
                                                </li>

                                                <li data-page="next" id="prev">
                                                    <span> > <span class="sr-only">(current)</span></span>
                                                </li>
                                            </ul>
                                        </nav>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="cal-history">
                            <div class="container-fluid">
                                <div class="row">

                                    <div class="col-xs-4 col-sm-4 col-md-5 col-lg-4">
                                        <div class="cal-log">
                                            <ul>
                                                <li>Number of Calls Attended :<b>250</b></li>
                                                <li>Number of Cases Solved :<b>250</b></li>
                                                <li>Average Call Duration :<b>100-00HRS</b></li>
                                                <li>Number of Patients Attended :<b>245</b></li>
                                            </ul>
                                        </div>
                                    </div>
                                    <div class="col-xs-4 col-sm-4 col-md-5 col-lg-5">
                                        <div class="cal-instructions">
                                            <ul>
                                                <li>Instructions</li>

                                                <div class="cal-instructions-content">
                                                    <p class="">
                                                        Lorem ipsum dolor sit amet, consectetur adipiscing elit,
                                                        sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.
                                                        Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat.
                                                        Duis aute irure dolor in reprehenderit in voluptate velit esse cillum dolore eu fugiat nulla pariatur.
                                                    </p>
                                                </div>
                                            </ul>
                                        </div>
                                    </div>
                                    <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1"></div>
                                </div>
                            </div>
                        </div>
                        <div class="rec-footer">
                            <div class="container-fluid">
                                <div class="row">
                                    <div class="col-xs-1 col-sm-1 col-md-5 col-lg-4">
                                        <div class="rec-footer-copyright">
                                            Copyright@2020 Telemedicine. All rights reserved.
                                        </div>
                                    </div>
                                    <div class="col-xs-1 col-sm-1 col-md-2 col-lg-5"></div>
                                    <div class="col-xs-1 col-sm-1 col-md-5 col-lg-3">
                                        <div class="rec-footer-terms">
                                            Privacy and policy | Terms & Conditions
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>

        </div>

    </div>
}

@section scripts {
    <script type="text/javascript">
        $(document).ready(function () {
            $("#loginUserName").text((window.localStorage.getItem('LoginUser')));
            $("#h3Heading").text("Reception Office");
            GetCallerDetails();
        });
        function GetCallerDetails() {
             var callerUrl = '@Url.Action("GetCallerDetails", "Reception")';
            $.ajax({
                type: "GET",
                async: false,
                contentType: "application/json; charset=utf-8",
                url: callerUrl,
                data: "",
                dataType: "json",
                //cache: false,
                success: function (response) {
                    $("#patientData tbody").empty();
                    $.each(response, function (index, item) {
                        var rowData = "<tr>:";
                        rowData += "<td> Sample Text</td>";
                        rowData += "<td>Sample Text Type Here</td>";
                        rowData += "<td>098765432</td>";
                        rowData += "<td>22-5-2020</td>";
                        rowData += "<td>Sample Text Type Here</td>";
                        rowData += "<td>19-15-20</td>";
                        rowData += "<td>";
                        rowData += "<i class='fas fa-mobile-alt grey-icon'></i>";
                        rowData += "<i class='far fa-times-circle red-icon'></i>";
                        rowData += "<i class='far fa-check-circle green-icon'></i>";
                        rowData += "</td>"
                        $("#patientData tbody").append(rowData);
                    });
                },
                error: function (result) {
                    alert("Error");
                    console.log(result);
                }
            });
        }
    </script>
}

